{
  "name": "mypage",
  "homepage": "https://yugaro.com",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 8888",
    "dev:aspida": "aspida --watch",
    "build": "aspida && next build",
    "start": "next start -p 8888",
    "lint": "eslint --fix '**/*.{ts,tsx,json}'",
    "lint:css": "stylelint --fix '**/*.{css,scss,sass}'",
    "api:build": "aspida",
    "export": "next export",
    "prepare": "husky install",
    "predeploy": "next build && next export"
  },
  "dependencies": {
    "@agney/react-loading": "^0.1.2",
    "@aspida/axios": "^1.9.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/gtag.js": "^0.0.10",
    "axios": "^0.26.1",
    "next": "12.1.5",
    "npm-run-all": "^4.1.5",
    "qs": "^6.10.3",
    "react": "18.0.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "18.0.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.0",
    "react-slick": "^0.29.0",
    "sass": "^1.50.1",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "17.0.24",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.5",
    "@types/react-anchor-link-smooth-scroll": "^1.0.2",
    "@types/react-dom": "18.0.1",
    "@types/react-modal": "^3.13.1",
    "@types/react-scroll": "^1.8.3",
    "@types/react-slick": "^0.23.8",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "8.13.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "prettier": "^2.6.2",
    "react-scroll": "^1.8.7",
    "serve": "^13.0.2",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.{js,jsx,ts,tsx,md}": "prettier --write",
    "src/**/*.{css,scss,sass}": "stylelint --fix"
  }
}
